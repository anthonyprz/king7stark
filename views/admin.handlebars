  <title>Administrador</title>
</head>

<body>
  <div class="btn-group" role="group" aria-label="Basic example">
    <button type="button" class="btn btn-outline-dark" data-toggle="modal" data-target="#gallery" data-whatever="@mdo">Add Gallery</button>
    <button type="button" class="btn btn-outline-dark" data-toggle="modal" data-target="#photo" data-whatever="@fat">Add Image</button>
  </div>

  <div class="modal fade" id="photo" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Add photo</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
        </div>
        <div class="modal-body">
          <form method="POST" action="/photo/insert">
            <div class="form-group">
              <label for="recipient-name" class="form-control-label">Gallery Name:</label>
              <input type="text" class="form-control" id="recipient-name" name="galleryname">
            </div>
            <div class="form-group">
              <label for="recipient-name" class="form-control-label">Photo Name:</label>
              <input type="text" class="form-control" id="recipient-name" name="photoname">
            </div>
            <div class="form-group">
              <label for="recipient-name" class="form-control-label">Photo Ruta:</label>
              <input type="text" class="form-control" id="recipient-name" name="photoruta">
            </div>
            <input type="submit" value="Submit" class="btn btn-outline-dark">
          </form>
        </div>
      </div>
    </div>
  </div>
  
  
    <div class="modal fade" id="gallery" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Add Gallery</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
        </div>
        <div class="modal-body">
          <form method="POST" action="/gallery/insert">
            <div class="form-group">
              <label for="recipient-name" class="form-control-label">Gallery Name:</label>
              <input type="text" class="form-control" id="recipient-name" name="galleryname">
            </div>
            <input type="submit" value="Submit" class="btn btn-outline-dark">
          </form>
        </div>
      </div>
    </div>
  </div>

  <article class="thumb">
    {{#each photo}}
    <a href="images/fulls/02.jpg" class="image"><img src="{{this.photoruta}}" alt="" /></a> {{/each}}
  </article>


  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
